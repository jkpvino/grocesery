<div id="emajaxblock_215a453913158e5c1d9dc430456c7d1e" class="ajaxblock_loading">
	<div class="ajaxLoad">
	</div>
</div>
<script type="text/javascript">// <![CDATA[
	var divContent = jQuery('#emajaxblock_215a453913158e5c1d9dc430456c7d1e').parents(".tab-content").first();	
	jQuery('a[href="#' + divContent.attr("id") + '"]').click(function(event){
            event.preventDefault();
            jQuery(this).parent().addClass("active");
            jQuery(this).parent().siblings().removeClass("active");
            var tab = jQuery(this).attr("href");
        	jQuery("#"+jQuery('#emajaxblock_215a453913158e5c1d9dc430456c7d1e').parents(".emajaxtabs").attr("id")+" .tab-content").not(tab).css("display", "none");
            jQuery(".emajaxtabs .tab-content").not(tab).removeClass("active");
            jQuery(tab).addClass("active");
            jQuery(tab).fadeIn("fast");
	});

	var tabContent = jQuery('#emajaxblock_215a453913158e5c1d9dc430456c7d1e').parents(".tab-content").first();
	if(tabContent.length > 0){
		var elTitle = jQuery('.custom-tab').find("a[data-tab='" + tabContent.attr("id") + "']").first();
		if(elTitle.parent(".item").hasClass("active"))
		{
			jQuery(elTitle.attr("href")).addClass("active");
			jQuery.ajax({
		        url: 'http://127.0.0.1/grocery/index.php/ajaxblock/?',
		        }).done(function(data) {
		            jQuery('#emajaxblock_215a453913158e5c1d9dc430456c7d1e').html(data);
					jQuery('#emajaxblock_215a453913158e5c1d9dc430456c7d1e').removeClass('ajaxblock_loading').addClass('ajaxblock_loaded');
		            jQuery(window).trigger("ajaxblock");
		    });
		}
		else{
			tabContent.hide();
			elTitle.bind("click",function () {
				var that = this;
				if(!jQuery(this).hasClass("loaded")){
	                jQuery.ajax({
	                 url: 'http://127.0.0.1/grocery/index.php/ajaxblock/?',
	                }).done(function(data) {
	                    jQuery(that).addClass("loaded");
	                    jQuery('#emajaxblock_215a453913158e5c1d9dc430456c7d1e').html(data);
	                    jQuery('#emajaxblock_215a453913158e5c1d9dc430456c7d1e').removeClass('ajaxblock_loading').addClass('ajaxblock_loaded');
	                    jQuery(window).trigger("ajaxblock");
	                });
	            }
			});
		}

	}
	
		
	
// ]]></script>